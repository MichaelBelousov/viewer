{
  "name": "@bentley/tree-widget-react",
  "version": "1.3.1",
  "description": "Tree Widget React",
  "keywords": [
    "Bentley",
    "iModel Tree Widget",
    "iModel.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/imodeljs/viewer-components-react/tree/master/packages/tree-widget"
  },
  "license": "MIT",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "main": "lib/tree-widget-react.js",
  "typings": "lib/tree-widget-react",
  "scripts": {
    "start": "concurrently npm:watch-build npm:watch-scss npm:watch-images",
    "watch-build": "concurrently \"npm:dev-build -- -w\" \"npm:prod-build -- -w\"",
    "watch-scss": "nodemon -e scss --watch src --exec \"npm run dev-build-scss\"",
    "watch-images": "nodemon -e * --watch src/images --exec \"npm run dev-copy-images\"",
    "build": "npm run dual-build && npm run build-scss && npm run copy-images && npm run copy-public",
    "dual-build": "concurrently npm:dev-build npm:prod-build",
    "dev-build": "tsc -p development.tsconfig.json",
    "prod-build": "tsc -b",
    "build-scss": "concurrently npm:dev-build-scss npm:prod-build-scss",
    "dev-build-scss": "copyfiles -u1 \"./src/**/*.scss\" ./build",
    "prod-build-scss": "copyfiles -u1 \"./src/**/*.scss\" ./lib",
    "copy-images": "concurrently npm:dev-copy-images npm:prod-copy-images",
    "dev-copy-images": "copyfiles -u1 \"./src/images/**\" ./build",
    "prod-copy-images": "copyfiles -u1 \"./src/images/**\" ./lib",
    "copy-public": "concurrently npm:dev-copy-public npm:prod-copy-public",
    "dev-copy-public": "copyfiles -u1 \"./public/**/*\" ./build/public",
    "prod-copy-public": "copyfiles -u1 \"./public/**/*\" ./lib/public",
    "prepare": "npm run build",
    "test": "jest",
    "test-watch": "jest --watch",
    "clean:build": "rimraf lib build",
    "rebuild": "npm run clean:build && npm run build"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "i18next": "^10.2.2",
    "react-redux": "^7.2.0",
    "redux": "^4.0.3"
  },
  "devDependencies": {
    "@bentley/bentleyjs-core": "^2.15.2",
    "@bentley/build-tools": "^2.15.2",
    "@bentley/context-registry-client": "^2.15.2",
    "@bentley/frontend-authorization-client": "^2.15.2",
    "@bentley/geometry-core": "^2.15.2",
    "@bentley/imodelhub-client": "^2.15.2",
    "@bentley/imodeljs-common": "^2.15.2",
    "@bentley/imodeljs-frontend": "^2.15.2",
    "@bentley/imodeljs-i18n": "^2.15.2",
    "@bentley/imodeljs-markup": "^2.15.2",
    "@bentley/imodeljs-quantity": "^2.15.2",
    "@bentley/itwin-client": "^2.15.2",
    "@bentley/orbitgt-core": "^2.15.2",
    "@bentley/presentation-common": "^2.15.2",
    "@bentley/presentation-components": "^2.15.2",
    "@bentley/presentation-frontend": "^2.15.2",
    "@bentley/product-settings-client": "^2.15.2",
    "@bentley/ui-abstract": "^2.15.2",
    "@bentley/ui-components": "^2.15.2",
    "@bentley/ui-core": "^2.15.2",
    "@bentley/ui-framework": "^2.15.2",
    "@bentley/ui-ninezone": "^2.15.2",
    "@bentley/webgl-compatibility": "^2.15.2",
    "@types/i18next": "^8.4.2",
    "@types/react": "~16.9.35",
    "@types/react-dom": "^16.8.0",
    "@types/react-redux": "^7.0.1",
    "cpx": "^1.5.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "rimraf": "^3.0.2",
    "tslint": "^5.11.0",
    "typescript": "~3.7.4",
    "concurrently": "^5.2.0",
    "copyfiles": "^2.1.0"
  },
  "peerDependencies": {
